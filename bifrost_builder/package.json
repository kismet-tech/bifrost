{
  "name": "bifrost_builder",
  "version": "1.814.0",
  "type": "module",
  "scripts": {
    "dev": "yarn upgrade_libraries && vite --mode preview",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy_experimental": "yarn upgrade_libraries && yarn version --minor --no-git-tag-version && SENTRY_ENV=experimental yarn build && gsutil -h 'Cache-Control:no-cache, no-store, max-age=0' cp dist/bifrost.iife.js gs://experimental_kismet_bifrost/bifrost.iife.js",
    "deploy_beta": "yarn upgrade_libraries && yarn version --minor --no-git-tag-version && SENTRY_ENV=beta yarn build && gsutil -h 'Cache-Control:no-cache, no-store, max-age=0' cp dist/bifrost.iife.js gs://beta_kismet_bifrost/bifrost.iife.js",
    "WARNING_deploy_prod": "yarn version --minor && SENTRY_ENV=prod yarn build && gsutil cp dist/bifrost.iife.js gs://kismet_bifrost/bifrost.js",
    "prestorybook": "yarn run upgrade_libraries",
    "upgrade_libraries": "yarn upgrade --latest @kismet_ai/aura@latest"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
