{
  "name": "bifrost_builder",
  "version": "1.129.0",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite --mode preview",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy_beta": "yarn version --minor --no-git-tag-version && SENTRY_ENV=beta yarn build && gsutil -h 'Cache-Control:no-cache, no-store, max-age=0' cp dist/bifrost.iife.js gs://beta_kismet_bifrost/bifrost.iife.js",
    "deploy_prod": "yarn version --minor && SENTRY_ENV=prod yarn build && gsutil cp dist/bifrost.iife.js gs://kismet_bifrost/bifrost.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cloud_build_storybook": "gcloud builds submit --config '.storybook/cloudbuild.yaml' --project playgroundist",
    "deploy_storybook": "yarn cloud_build_storybook && gcloud run deploy kismet-bifrost-storybook --region us-central1 --image us-docker.pkg.dev/playgroundist/kismet-docker-repo/kismet-bifrost-storybook:latest"
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@sentry/browser": "^8.30.0",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "storybook": "^8.3.0",
    "tailwindcss": "^3.4.12",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-css-injected-by-js": "^3.5.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  }
}
