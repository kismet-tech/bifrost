    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/themes": "^3.1.6",
    "@sentry/browser": "^8.30.0",
    "@tanstack/react-query": "^5.60.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.3.0",
    "inputmask": "^5.0.9",
